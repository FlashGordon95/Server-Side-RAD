<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
			
	
	
	<bean id="student" class="com.sample12.Student" >
	
	<constructor-arg name="firstName" value ="Jim" ></constructor-arg>
	<constructor-arg name="surname" value ="Haughey" ></constructor-arg>
	<constructor-arg name="age" value ="33" ></constructor-arg>
	
	</bean>
	
	
	

</beans>
