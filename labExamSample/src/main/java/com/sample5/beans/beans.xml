<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	
	
	
	
	
	<bean id="student" class="com.sample5.Student" autowire="byName">
	</bean>
	
	<bean id="address1" class="com.sample5.Address">
	<constructor-arg name="town" value ="Cong" ></constructor-arg>
	<constructor-arg name="county" value ="Mo" ></constructor-arg>
	</bean>
	
	<bean id="address" class="com.sample5.Address">
	<constructor-arg name="town" value ="Tuam" ></constructor-arg>
	<constructor-arg name="county" value ="GY" ></constructor-arg>
	</bean>
	

</beans>
